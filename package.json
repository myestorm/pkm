{
  "name": "totonoo-pkm",
  "version": "0.0.2",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env prd",
    "start-docker": "pm2 start ecosystem.config.js --env prd --no-daemon",
    "install": "npm run app-build && node ./www/install",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Vite,TypeScript\" -c \"yellow.bold,cyan.bold\" \"npm run vite-dev\" \"npm run app-dev\"",
    "dev_1": "npm run app-build && concurrently -k -p \"[{name}]\" -n \"Vite,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run vite-dev\" \"npm run app-watch\" \"npm run app-dev\"",
    "build": "npm run vite-build && npm run app-build",
    "vite-dev": "vite --host",
    "vite-build": "vue-tsc --noEmit && vite build",
    "vite-serve": "vite preview",
    "app-dev": "nodemon",
    "app-watch": "tsc -p tsconfig.app.json -w",
    "app-build": "tsc -p tsconfig.app.json",
    "test:app": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/app/**/*.test.ts'",
    "test:src": "vitest"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@totonoo/vue-codemirror": "^1.1.6",
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.1",
    "ejs": "^3.1.7",
    "formidable": "^2.0.1",
    "glob": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "minimatch": "^5.0.1",
    "mongoose": "^6.3.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.25.0",
    "@types/chai": "^4.3.1",
    "@types/crypto-js": "^4.1.1",
    "@types/ejs": "^3.1.0",
    "@types/formidable": "^2.0.4",
    "@types/glob": "^7.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-static": "^4.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/mocha": "^9.1.1",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.6.0",
    "@types/supertest": "^2.0.12",
    "@types/svgo": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.21",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.38",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "sass": "^1.50.1",
    "supertest": "^6.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-pwa": "^0.12.0",
    "vite-svg-loader": "^3.2.0",
    "vitest": "^0.10.0",
    "vue-tsc": "^0.34.10"
  }
}
