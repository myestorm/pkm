{
  "name": "totonoo-pkm",
  "version": "0.0.2",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env prd",
    "start-docker": "pm2 start ecosystem.config.js --env prd --no-daemon",
    "dev": "npm run app-build && concurrently -k -p \"[{name}]\" -n \"Vite,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run vite-dev\" \"npm run app-watch\" \"npm run app-nodemon\"",
    "build": "npm run vite-build && npm run app-build",
    "vite-dev": "vite --host",
    "vite-build": "vue-tsc --noEmit && vite build",
    "vite-serve": "vite preview",
    "app-watch": "tsc -p tsconfig.app.json -w",
    "app-nodemon": "nodemon",
    "app-build": "tsc -p tsconfig.app.json",
    "app-install": "npm run app-build && node ./www/app/install"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@lezer/markdown": "^0.15.6",
    "@totonoo/vue-codemirror": "^1.1.4",
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.0",
    "ejs": "^3.1.6",
    "glob": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "markdown-it": "^12.3.2",
    "mongoose": "^6.2.11",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.23.0",
    "@types/crypto-js": "^4.1.1",
    "@types/ejs": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-static": "^4.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.6.0",
    "@types/svgo": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "less": "^4.1.2",
    "nodemon": "^2.0.15",
    "sass": "^1.50.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.4",
    "vite-plugin-pwa": "^0.11.13",
    "vite-svg-loader": "^3.2.0",
    "vue-tsc": "^0.34.6"
  }
}
